ort=\"backtofront\" metadata=\"alpha\" /\u0026gt;\n    \u0026lt;command type=\"scenepass\" pass=\"postalpha\" sort=\"backtofront\" /\u0026gt;\n\u0026lt;/renderpath\u0026gt;\n\u003c/code\u003e\u003c/pre\u003e","post_number":6,"post_type":1,"updated_at":"2017-10-30T21:03:49.280Z","reply_count":1,"reply_to_post_number":5,"quote_count":0,"incoming_link_count":0,"reads":35,"readers_count":34,"score":72.0,"yours":false,"topic_id":3691,"topic_slug":"screenshotting-image-leaving-background-transparent","display_username":"SirNate0","primary_group_name":null,"flair_name":null,"flair_url":null,"flair_bg_color":null,"flair_color":null,"version":1,"can_edit":false,"can_delete":false,"can_recover":false,"can_wiki":false,"read":true,"user_title":null,"reply_to_user":{"username":"jmiller","name":"Justin Miller","avatar_template":"/user_avatar/discourse.urho3d.io/jmiller/{size}/2367_2.png"},"bookmarked":false,"actions_summary":[{"id":2,"count":2}],"moderator":false,"admin":false,"staff":false,"user_id":628,"hidden":false,"trust_level":2,"deleted_at":null,"user_deleted":false,"edit_reason":null,"can_view_edit_history":true,"wiki":false,"can_accept_answer":false,"can_unaccept_answer":false,"accepted_answer":true},{"id":21496,"name":"","username":"ppsychrite","avatar_template":"/user_avatar/discourse.urho3d.io/ppsychrite/{size}/975_2.png","created_at":"2017-10-30T12:30:39.369Z","cooked":"\u003cp\u003eNo, I call SavePNG() directly instead of JPG.\u003cbr\u003e\nhmm thanks for the RenderPath I‚Äôll try it after school. \u003cimg src=\"https://emoji.discourse-cdn.com/twitter/smile.png?v=5\" title=\":smile:\" class=\"emoji\" alt=\":smile:\"\u003e\u003c/p\u003e","post_number":7,"post_type":1,"updated_at":"2017-10-30T12:30:39.369Z","reply_count":0,"reply_to_post_number":6,"quote_count":0,"incoming_link_count":1,"reads":32,"readers_count":31,"score":11.4,"yours":false,"topic_id":3691,"topic_slug":"screenshotting-image-leaving-background-transparent","display_username":"","primary_group_name"