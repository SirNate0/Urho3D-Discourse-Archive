  {\n\u003c/li\u003e\n\u003cli\u003e        dir = GetInternalPath(String(prefPath));\n\u003c/li\u003e\n\u003cli\u003e        SDL_free(prefPath);\n\u003c/li\u003e\n\u003cli\u003e    }\n\u003c/li\u003e\n\u003cli\u003e    else\n\u003c/li\u003e\n\u003cli\u003e#endif\n\u003c/li\u003e\n\u003cli\u003e        URHO3D_LOGWARNING(\"Could not get application preferences directory\");\n\u003c/li\u003e\n\u003cli\u003e\n\u003c/li\u003e\n\u003cli\u003e    return dir;\n\u003c/li\u003e\n\u003cli\u003e}\n\u003c/li\u003e\n\u003c/ol\u003e\u003c/code\u003e\u003c/pre\u003e\n\n\n  \u003c/article\u003e\n  \u003cdiv class=\"onebox-metadata\"\u003e\n    \n    \n  \u003c/div\u003e\n  \u003cdiv style=\"clear: both\"\u003e\u003c/div\u003e\n\u003c/aside\u003e\n\u003c/p\u003e","post_number":2,"post_type":1,"updated_at":"2019-08-13T15:01:19.986Z","reply_count":1,"reply_to_post_number":null,"quote_count":0,"incoming_link_count":0,"reads":26,"readers_count":25,"score":10.2,"yours":false,"topic_id":5460,"topic_slug":"could-not-get-application-preferences-directory","display_username":"é­”å¤§å†œ ğğğ³ ç¾æ‹›èœ","primary_group_name":null,"flair_name":null,"flair_url":null,"flair_bg_color":null,"flair_color":null,"version":1,"can_edit":false,"can_delete":false,"can_recover":false,"can_wiki":false,"link_counts":[{"url":"https://github.com/urho3d/Urho3D/blob/master/Source/Urho3D/IO/FileSystem.cpp#L742-L757","internal":false,"reflection":false,"title":"Urho3D/FileSystem.cpp at master Â· urho3d/Urho3D Â· GitHub","clicks":0}],"read":true,"user_title":"","bookmarked":false,"actions_summary":[],"moderator":false,"admin":false,"staff":false,"user_id":192,"hidden":false,"trust_level":0,"deleted_at":null,"user_deleted":false,"edit_reason":null,"can_view_edit_history":true,"wiki":false,"user_suspended":true,"can_accept_answer":false,"can_unaccept_answer":false,"accepted_answer":false},{"id":32522,"name":"Capel English","username":"capelenglish","avatar_template":"https://avatars.discourse-cdn.com/v4/letter/c/e47774/{