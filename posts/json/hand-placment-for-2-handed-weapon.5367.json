io/modanung/{size}/3290_2.png"},"bookmarked":false,"actions_summary":[{"id":2,"count":1}],"moderator":false,"admin":false,"staff":false,"user_id":1004,"hidden":false,"trust_level":0,"deleted_at":null,"user_deleted":false,"edit_reason":null,"can_view_edit_history":true,"wiki":false,"user_suspended":true,"can_accept_answer":false,"can_unaccept_answer":false,"accepted_answer":false},{"id":31575,"name":"","username":"GodMan","avatar_template":"https://avatars.discourse-cdn.com/v4/letter/g/e79b87/{size}.png","created_at":"2019-07-30T15:04:10.801Z","cooked":"\u003cp\u003eIs this in the samples?\u003c/p\u003e","post_number":4,"post_type":1,"updated_at":"2019-07-30T15:04:10.801Z","reply_count":1,"reply_to_post_number":null,"quote_count":0,"incoming_link_count":0,"reads":39,"readers_count":38,"score":12.8,"yours":false,"topic_id":5367,"topic_slug":"hand-placment-for-2-handed-weapon","display_username":"","primary_group_name":null,"flair_name":null,"flair_url":null,"flair_bg_color":null,"flair_color":null,"version":1,"can_edit":false,"can_delete":false,"can_recover":false,"can_wiki":false,"read":true,"user_title":null,"bookmarked":false,"actions_summary":[],"moderator":false,"admin":false,"staff":false,"user_id":861,"hidden":false,"trust_level":2,"deleted_at":null,"user_deleted":false,"edit_reason":null,"can_view_edit_history":true,"wiki":false,"can_accept_answer":false,"can_unaccept_answer":false,"accepted_answer":false},{"id":31580,"name":"È≠îÂ§ßÂÜú ùûçùûéùù≥ ÁèæÊãõËúç","username":"Modanung","avatar_template":"/user_avatar/discourse.urho3d.io/modanung/{size}/3290_2.png","created_at":"2019-07-30T17:54:28.244Z","cooked":"\u003cp\u003eHave you seen \u003cem\u003esample 45\u003c/em\u003e?\u003c/p\u003e","post_number":5,"post_type":1,"updated_at":"2019-07-30T17:54:28.244Z","reply_count":0,"reply_to_post_number":4,"quote_count":0,"incoming_link_count":0,"reads":39,"readers_count":38,"score":7.8,"yours":false,"topic_id":5367,"topic_slug":"hand-placment-for-2-handed-weapon","display_username":"È≠îÂ§ßÂÜú ùûçùûéùù≥ ÁèæÊãõËúç","primary_group_name":null,"flair_name":null,"flair_url":null,"flair_bg_color":null,"flair_color":null,"version":1,"can_edit":false,"can_delete":false,"can_recover":false,"can_wiki":false,"read":true,"user_title":"","reply_to_user":{"username":"GodMan","name":"","avatar_template":"https://avatars.discourse-cdn.com/v4/letter/g/e79b87/{size}.png"},"bookmarked":false,"actions_summary":[],"moderator":false,"admin":false,"staff":false,"user_id":192,"hidden":false,"trust_level":0,"deleted_at":null,"user_deleted":false,"edit_reason":null,"can_view_edit_history":true,"wiki":false,"user_suspended":true,"can_accept_answer":false,"can_unaccept_answer":false,"accepted_answer":false},{"id":31581,"name":"","username":"TheComet","avatar_template":"/user_avatar/discourse.urho3d.io/thecomet/{size}/1493_2.png","created_at":"2019-07-30T19:42:21.260Z","cooked":"\u003cp\u003eThe IK components should be of use to you. See here for an overview: \u003ca href=\"https://urho3d.github.io/documentation/1.7/_i_k.html\" rel=\"nofollow noopener\"\u003ehttps://urho3d.github.io/documentation/1.7/_i_k.html\u003c/a\u003e\u003c/p\u003e\n\u003cp\u003eIf you have any questions or need help getting it to work we‚Äôre happy to help!\u003c/p\u003e","post_number":6,"post_type":1,"updated_at":"2019-07-30T22:04:55.895Z","reply_count":0,"reply_to_post_number":null,"quote_count":0,"incoming_link_count":1,"reads":37,"readers_count":36,"score":42.4,"yours":false,"topic_id":5367,"topic_slug":"hand-placment-for-2-handed-weapon","display_username":"","primary_group_name":null,"flair_name":null,"flair_url":null,"flair_bg_color":null,"flair_color":null,"version":2,"can_edit":false,"can_delete":false,"can_recover":false,"can_wiki":false,"link_counts":[{"url":"https://urho3d.github.io/documentation/1.7/_i_k.html","internal":false,"reflection":false,"title":"Urho3D - Documentation - Inverse Kinematics","clicks":3}],"read":true,"user_title":"Community repository maintainer","title_is_group":false,"bookmarked":false,"actions_summary":[{"id":2,"count":2}],"moderator":false,"admin":false,"staff":false,"user_id":37,"hidden":false,"trust_level":2,"deleted_at":null,"user_deleted":false,"edit_reason":null,"can_view_edit_history":true,"wiki":false,"can_accept_answer":false,"can_unaccept_answer":false,"accepted_answer":true},{"id":31589,"name":"","username":"GodMan","avatar_template":"https://avatars.discourse-cdn.com/v4/letter/g/e79b87/{size}.png","created_at":"2019-07-30T23:23:13.504Z","cooked":"\u003cp\u003eOkay thanks guys. I will look at sample 45 as well.\u003c/p\u003e","post_number":7,"post_type":1,"updated_at":"2019-07-30T23:23:13.504Z","reply_count":0,"reply_to_post_number":null,"quote_count":0,"incoming_link_count":0,"reads":33,"readers_count":32,"score":6.6,"yours":false,"topic_id":5367,"topic_slug":"hand-placment-for-2-handed-weapon","display_username":"","primary_group_name":null,"flair_name":null,"flair_url":null,"flair_bg_color":null,"flair_color":null,"version":1,"can_edit":false,"can_delete":false,"can_recover":false,"can_wiki":false,"read":true,"user_title":null,"bookmarked":false,"actions_summary":[],"moderator":false,"admin":false,"staff":false,"user_id":861,"hidden":false,"trust_level":2,"deleted_at":null,"user_deleted":false,"edit_reason":null,"can_view_edit_history":true,"wiki":false,"can_accept_answer":false,"can_unaccept_answer":false,"accepted_answer":false},{"id":31595,"name":"Leith Ketchell","username":"Leith","avatar_template":"/user_avatar/discourse.urho3d.io/leith/{size}/2384_2.png","created_at":"2019-07-31T05:30:15.051Z","cooked":"\u003cp\u003eHi, GodMan!\u003c/p\u003e\n\u003cp\u003eI recently dealt with this issue, it involved a two-handed axe attack‚Ä¶ and a solution came about rather accidentally‚Ä¶\u003cbr\u003e\nI got around this issue without using IK on the arms.\u003cbr\u003e\nHere‚Äôs a brief description of how I achieved it‚Ä¶\u003c/p\u003e\n\u003cp\u003eI created a locator node as a child of one of the hands.\u003cbr\u003e\nI attached my weapon to that node, and used it to orient and position the weapon relative to the hand.\u003cbr\u003e\nThereafter, no matter what animation was happening, the weapon would be glued to that hand‚Ä¶\u003cbr\u003e\nFor two handed attacks, I used animations that suggest both hands become attached to the weapon, but they are simply crafted to appear so - it‚Äôs visually plausible, and cheaper / more stable than IK.\u003c/p\u003e\n\u003cp\u003e‚ÄúIf the hands are oriented and positioned appropriately during animation, and the axe is glued to one of the hands, then the axe will naturally follow the hand to which it is truly attached‚Äù\u003c/p\u003e\n\u003cp\u003eI had some fun swapping out the axe for a teapot, the Urho Fish model, and various other things - the same attack animation, where the hands come together, we swing, then the hands come apart, one attack animation, worked for any number of weapons we could potentially use in a two handed attack.\u003c/p\u003e\n\u003cp\u003eBy all means, try IK, but be warned there are some caveats with respect to our current implementation of IK, including that Scale in the node hierarchy and/or animations causes crazy things to happen to the IK and to its debugdrawing as well.\u003c/p\u003e","post_number":8,"post_type":1,"updated_at":"2019-07-31T06:50:33.748Z","reply_count":0,"reply_to_post_number":null,"quote_count":0,"incoming_link_count":0,"reads":29,"readers_count":28,"score":20.8,"yours":false,"topic_id":5367,"topic_slug":"hand-placment-for-2-handed-weapon","display_username":"Leith Ketchell","primary_group_name":null,"flair_name":null,"flair_url":null,"flair_bg_color":null,"flair_color":null,"version":3,"can_edit":false,"can_delete":false,"can_recover":false,"can_wiki":false,"read":true,"user_title":"suspended user","title_is_group":false,"bookmarked":false,"actions_summary":[{"id":2,"count":1}],"moderator":false,"admin":false,"staff":false,"user_id":1098,"hidden":false,"trust_level":1,"deleted_at":null,"user_deleted":false,"edit_reason":null,"can_view_edit_history":true,"wiki":false,"user_suspended":true,"can_accept_answer":false,"can_unaccept_answer":false,"accepted_answer":false},{"id":31728,"name":"","username":"GodMan","avatar_template":"https://avatars.discourse-cdn.com/v4/letter/g/e79b87/{size}.png","created_at":"2019-07-31T19:31:02.254Z","cooked":"\u003cp\u003e\u003ca class=\"mention\" href=\"/u/leith\"\u003e@Leith\u003c/a\u003e I have done that in the past just like you did, but this won‚Äôt work in this case because the hammer handle is pretty long so you can see the AI‚Äôs right hand is off when he swings the hammer. It‚Äôs the gravity hammer from halo.\u003c/p\u003e","post_number":9,"post_type":1,"updated_at":"2019-07-31T19:31:02.254Z","reply_count":1,"reply_to_post_number":null,"quote_count":0,"incoming_link_count":0,"reads":26,"readers_count":25,"score":10.2,"yours":false,"topic_id":5367,"topic_slug":"hand-placment-for-2-handed-weapon","display_username":"","primary_group_name":null,"flair_name":null,"flair_url":null,"flair_bg_color":null,"flair_color":null,"version":1,"can_edit":false,"can_delete":false,"can_recover":false,"can_wiki":false,"read":true,"user_title":null,"bookmarked":false,"actions_summary":[],"moderator":false,"admin":false,"staff":false,"user_id":861,"hidden":false,"trust_level":2,"deleted_at":null,"user_deleted":false,"edit_reason":null,"can_view_edit_history":true,"wiki":false,"can_accept_answer":false,"can_unaccept_answer":false,"accepted_answer":false},{"id":31742,"name":"Leith Ketchell","username":"Leith","avatar_template":"/user_avatar/discourse.urho3d.io/leith/{size}/2384_2.png","created_at":"2019-08-01T06:41:17.006Z","cooked":"\u003cp\u003eFair enough!\u003cimg src=\"https://emoji.discourse-cdn.com/twitter/slight_smile.png?v=9\" title=\":slight_smile:\" class=\"emoji\" alt=\":slight_smile:\"\u003e\u003cbr\u003e\nIf you need accurate hand placement, you have two options - one is to use IK (I recommend sticking with two-bone solvers as they seem to be both fastest and most stable), the other is to adjust your animations, using an imposter axe handle object to model what will happen at runtime. That of course means that the animation is tied to that weapon, but in practice I tend to find that is the relationship we end up with anyway.\u003c/p\u003e","post_number":10,"post_type":1,"updated_at":"2019-08-01T06:41:17.006Z","reply_count":1,"reply_to_post_number":9,"quote_count":0,"incoming_link_count":0,"reads":24,"readers_count":23,"score":9.8,"yours":false,"topic_id":5367,"topic_slug":"hand-placment-for-2-handed-weapon","display_username":"Leith Ketchell","primary_group_name":null,"flair_name":null,"flair_url":null,"flair_bg_color":null,"flair_color":null,"version":1,"can_edit":false,"can_delete":false,"can_recover":false,"can_wiki":false,"read":true,"user_title":"suspended user","title_is_group":false,"reply_to_user":{"username":"GodMan","name":"","avatar_template":"https://avatars.discourse-cdn.com/v4/letter/g/e79b87/{size}.png"},"bookmarked":false,"actions_summary":[],"moderator":false,"admin":false,"staff":false,"user_id":1098,"hidden":false,"trust_level":1,"deleted_at":null,"user_deleted":false,"edit_reason":null,"can_view_edit_history":true,"wiki":false,"user_suspended":true,"can_accept_answer":false,"can_unaccept_answer":false,"accepted_answer":false},{"id":31743,"name":"","username":"George1","avatar_template":"https://avatars.discourse-cdn.com/v4/letter/g/9e8a1a/{size}.png","created_at":"2019-08-01T06:54:01.535Z","cooked":"\u003cp\u003eAnother idea\u003c/p\u003e\n\u003cp\u003eAttach two nodes to the weapon if two handed is needed.\u003cbr\u003e\nAttached a node to the hand where the weapon will be attached to.\u003c/p\u003e\n\u003cp\u003eAttach one of the node on the weapon to one of the hand.\u003c/p\u003e\n\u003cp\u003eNext do ik on the second hand to the second node on the weapon. This way less chance of error.\u003c/p\u003e","post_number":11,"post_type":1,"updated_at":"2019-08-01T06:54:01.535Z","reply_count":2,"reply_to_post_number":10,"quote_count":0,"incoming_link_count":0,"reads":24,"readers_count":23,"score":14.8,"yours":false,"topic_id":5367,"topic_slug":"hand-placment-for-2-handed-weapon","display_username":"","primary_group_name":null,"flair_name":null,"flair_url":null,"flair_bg_color":null,"flair_color":null,"version":1,"can_edit":false,"can_delete":false,"can_recover":false,"can_wiki":false,"read":true,"user_title":null,"reply_to_user":{"username":"Leith","name":"Leith Ketchell","avatar_template":"/user_avatar/discourse.urho3d.io/leith/{size}/2384_2.png"},"bookmarked":false,"actions_summary":[],"moderator":false,"admin":false,"staff":false,"user_id":627,"hidden":false,"trust_level":2,"deleted_at":null,"user_deleted":false,"edit_reason":null,"can_view_edit_history":true,"wiki":false,"can_accept_answer":false,"can_unaccept_answer":false,"accepted_answer":false},{"id":31745,"name":"Leith Ketchell","username":"Leith","avatar_template":"/user_avatar/discourse.urho3d.io/leith/{size}/2384_2.png","created_at":"2019-08-01T06:57:41.812Z","cooked":"\u003cp\u003eFor IK, you‚Äôre going to need two attachment points, but for a dedicated animation you only need one.\u003cbr\u003e\nIK is a more flexible solution, when applied after animation as a corrective measure, and my experiences using IK for foot placement have generally been positive‚Ä¶ yes you have a point, you really only need IK on the second hand, given the first hand is already attached‚Ä¶\u003c/p\u003e","post_number":12,"post_type":1,"updated_at":"2019-08-01T06:59:29.393Z","reply_count":0,"reply_to_post_number":11,"quote_count":0,"incoming_link_count":0,"reads":23,"readers_count":22,"score":4.6,"yours":false,"topic_id":5367,"topic_slug":"hand-placment-for-2-handed-weapon","display_username":"Leith Ketchell","primary_group_name":null,"flair_name":null,"flair_url":null,"flair_bg_color":null,"flair_color":null,"version":1,"can_edit":false,"can_delete":false,"can_recover":false,"can_wiki":false,"read":true,"user_title":"suspended user","title_is_group":false,"reply_to_user":{"username":"George1","name":"","avatar_template":"https://avatars.discourse-cdn.com/v4/letter/g/9e8a1a/{size}.png"},"bookmarked":false,"actions_summary":[],"moderator":false,"admin":false,"staff":false,"user_id":1098,"hidden":false,"trust_level":1,"deleted_at":null,"user_deleted":false,"edit_reason":null,"can_view_edit_history":true,"wiki":false,"user_suspended":true,"can_accept_answer":false,"can_unaccept_answer":false,"accepted_answer":false},{"id":31873,"name":"Ka-Wiz","username":"Ka-Wiz","avatar_template":"https://avatars.discourse-cdn.com/v4/letter/k/cdc98d/{size}.png","created_at":"2019-08-01T15:06:20.541Z","cooked":"\u003cp\u003eI studied how things worked in Chivalry: Medieval Warfare for awhile when I was working on a sword game. Their (amazing) animations are baked in the 3D software using the technique Leith is describing, and thus different for every class of weapon (but not every weapon)\u003c/p\u003e\n\u003cp\u003eBecause of the possible errors in IK, I think baking is the way to go. It‚Äôs really a choice of where you want your work to be, in content generation (3D) or coding (IK). Generally I think a baked animation is more likely to look good, but it‚Äôs also really hard to animate well if you‚Äôre not experienced in it (another lesson from that sword project‚Ä¶)\u003c/p\u003e","post_number":13,"post_type":1,"updated_at":"2019-08-01T15:06:20.541Z","reply_count":0,"reply_to_post_number":null,"quote_count":0,"incoming_link_count":0,"reads":24,"readers_count":23,"score":19.8,"yours":false,"topic_id":5367,"topic_slug":"hand-placment-for-2-handed-weapon","display_username":"Ka-Wiz","primary_group_name":null,"flair_name":null,"flair_url":null,"flair_bg_color":null,"flair_color":null,"version":1,"can_edit":false,"can_delete":false,"can_recover":false,"can_wiki":false,"read":true,"user_title":null,"bookmarked":false,"actions_summary":[{"id":2,"count":1}],"moderator":false,"admin":false,"staff":false,"user_id":827,"hidden":false,"trust_level":2,"deleted_at":null,"user_deleted":false,"edit_reason":null,"can_view_edit_history":true,"wiki":false,"can_accept_answer":false,"can_unaccept_answer":false,"accepted_answer":false},{"id":31883,"name":"","username":"GodMan","avatar_template":"https://avatars.discourse-cdn.com/v4/letter/g/e79b87/{size}.png","created_at":"2019-08-01T21:55:37.346Z","cooked":"\u003cp\u003eThats actually what I am doing. I have already added a marker for the AI‚Äôs right hand to the hammer.\u003c/p\u003e","post_number":14,"post_type":1,"updated_at":"2019-08-01T21:55:37.346Z","reply_count":1,"reply_to_post_number":11,"quote_count":0,"incoming_link_count":0,"reads":20,"readers_count":19,"score":9.0,"yours":false,"topic_id":5367,"topic_slug":"hand-placment-for-2-handed-weapon","display_username":"","primary_group_name":null,"flair_name":null,"flair_url":null,"flair_bg_color":null,"flair_color":null,"version":1,"can_edit":false,"can_delete":false,"can_recover":false,"can_wiki":false,"read":true,"user_title":null,"reply_to_user":{"username":"George1","name":"","avatar_template":"https://avatars.discourse-cdn.com/v4/letter/g/9e8a1a/{size}.png"},"bookmarked":false,"actions_summary":[],"moderator":false,"admin":false,"staff":false,"user_id":861,"hidden":false,"trust_level":2,"deleted_at":null,"user_deleted":false,"edit_reason":null,"can_view_edit_history":true,"wiki":false,"can_accept_answer":false,"can_unaccept_answer":false,"accepted_answer":false},{"id":31893,"name":"Leith Ketchell","username":"Leith","avatar_template":"/user_avatar/discourse.urho3d.io/leith/{size}/2384_2.png","created_at":"2019-08-02T05:50:45.386Z","cooked":"\u003cp\u003eHere‚Äôs a quick video that shows a two handed attack animation that I added a weapon to - no ik was used (including the feet! I extracted motion data from the animation), and I was able to convincingly swap out the weapon for new, unintended ones\u003cbr\u003e\nIt‚Äô